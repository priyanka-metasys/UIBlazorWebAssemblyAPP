@inject HttpClient httpClient

<MudMainContent Class="pt-0 px-0">

<MudPaper Class="mud-theme-default rounded-0 shadow-none" Style="background-color:white; overflow:hidden; position:relative;">

    <MudDrawerContainer Class="mud-height-full">
   
        <div class="">
            <div class="firstTab">
                <MudTabs Elevation="0" Rounded="true" ApplyEffectsToContainer="true"  @bind-ActivePanelIndex="activeIndex">
                    <MudTabPanel Text="Active (99)" >
                        <MudText>d</MudText>
                    </MudTabPanel>
                    <MudTabPanel Text="Section" >
                        <MudPaper>
                                <MudChip Disabled="true" Style="background-color:#5E5E5E;opacity: 1;" Color="Color.Dark">Printavo Acct: Is TRUE </MudChip>
                                <MudChip Style="background-color:#5E5E5E;opacity: 1;" Icon="@Icons.Filled.FilterList" Color="Color.Primary">  Add Filter</MudChip>
                                
                            </MudPaper>
                            <MudDataGrid T="Element" FixedHeader="@fixed_header" FixedFooter="@fixed_footer" Height="@(fixed_header || fixed_footer ?"710px":"")" Items="@Elements" Filterable="true" FilterMode="@_filterMode" FilterCaseSensitivity="@_caseSensitivity" Hover="true" Dense="true">
                            <Columns>
                                <Column T="Element" Field="Number" Title="Nr" Filterable="false" />
                                <Column T="Element" Field="Sign"  />
                                <Column T="Element" Field="Name" />
                                <Column T="Element" Field="Position" />
                                <Column T="Element" Field="Molar" Title="Molar mass" />
                                <Column T="Element" Field="Group" Title="Category" />
                            </Columns>
                            <PagerContent>
                                <MudDataGridPager T="Element" />
                            </PagerContent>
                        </MudDataGrid>
                    </MudTabPanel>
                    <MudTabPanel Text="+ Save View" Disabled="true">
                        <MudText>dv</MudText>
                    </MudTabPanel>
                </MudTabs>
            </div>           
        </div>
    </MudDrawerContainer>
</MudPaper>
</MudMainContent>
